@page "/"
@inject NavigationManager navigationManager

<h1>Loading....</h1>

@code {
	protected async override Task OnInitializedAsync()
	{
		try
		{
			await Task.Run(() => navigationManager.NavigateTo("/login"));
		}
		catch (Exception ex)
		{
			Console.WriteLine($"Error: {ex.Message}");
		}
	}
}